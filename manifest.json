{
  "manifest_version": 3,
  "name": "Tab Tracker",
  "version": "1.0",
  "description": "Track and organize all open browser tabs",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "assets/icon16.svg",
      "48": "assets/icon48.svg",
      "128": "assets/icon128.svg"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/icon16.svg",
    "48": "assets/icon48.svg",
    "128": "assets/icon128.svg"
  }
}
